        $("#guardaEstacion").on('click',function(){
            var idC=document.getElementById('idEstacion').value;
            var responsable=document.getElementById('responsable').value;
            var departamento=document.getElementById('departamento').value;
            var antivirus=document.getElementById('antivirus').value;
            var ip=document.getElementById('ip').value;
            var nombreEstacion=document.getElementById('nombreRed').value;
            var pass=document.getElementById('password').value;


            $.ajax({
                type: "POST",
                url:"{{ path('editar_estacionAjax')}}",
                data:
                    {
                        id:idC,
                        responsable:responsable,
                        passSetup:pass,
                        ip:ip,
                        nombreRed:nombreEstacion,
                        centroCosto:departamento,
                        antivirus:antivirus
                    }
                ,
                dataType: "json",
                success: function(response) {
                    console.log("La estacion ha sido correctamente actualizado");
                    $("#estabilizadorOk").fadeIn('500');
                    $("#estabilizadorOk").fadeOut('600');
                    $("#responsable").attr('disabled',true);
                    $("#departamento").attr('disabled',true);
                    $("#antivirus").attr('readonly',true);
                    $("#ip").attr('readonly',true);
                    $("#nombreRed").attr('readonly',true);
                    $("#password").attr('readonly',true);
                    document.getElementById("editaDatosEstacion").style.display = "initial";
                    document.getElementById("guardaEstacion").style.display = "none";
                    document.getElementById("cancelaEstacion").style.display = "none";
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(xhr.status);
                    alert(thrownError);
                }
            });
        });
    </script>
