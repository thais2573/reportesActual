{% block stylesheets %}
{% endblock %}

{% block body %}
<table border="1" cellpadding="1" cellspacing="1" style="width:900px;font-size: 12px">
  <tbody>
  <tr style="height: 20px">
    <td colspan="5" rowspan="1">ORGANISMO: MINSAP</td>
    <td style="text-align: center; font-weight: bold" colspan="5" rowspan="2">SOLICITUD DE MATERIALES</td>
  </tr>
  <tr style="height: 20px">
    <td colspan="5" rowspan="1">EMPRESA: "CICC"</td>
  </tr>
  <tr style="height: 20px">
    <td colspan="10">ALMACEN AL QUE SE SOLICITAN: Central</td>
  </tr>
  <tr style="height: 20px">
    <td>DESTINO</td>
    <td style="vertical-align: top" colspan="3" rowspan="1">ORDEN DE TRABAJO #:</td>
    <td style="vertical-align: top" colspan="4" rowspan="1">CENTRO DE COSTO:{{ lista.centroCosto }}</td>
    <td style="vertical-align: top" colspan="2" rowspan="1">CODIGO C.C:</td>
  </tr>
  <tr style="height: 20px">
    <td style="text-align: center" colspan="2" rowspan="1">CODIGO</td>
    <td style="text-align: center" colspan="6" rowspan="1">DESCRIPCION</td>
    <td style="text-align: center">U.M</td>
    <td style="text-align: center">CANTIDAD</td>
  </tr>
  {% for m in lista.material %}
  <tr style="height: 20px">
    <td style="text-align: center" colspan="2" rowspan="1">{{  m.idProducto.idProducto }}</td>
    <td style="text-align: center" colspan="6" rowspan="1">{{ m.idProducto.descripcion }}</td>
    <td style="text-align: center">{{ m.um }}</td>
    <td style="text-align: center">{{ m.cantidad }}</td>
  </tr>
{% endfor %}

  <tr style="height: 20px">
    <td style="text-align: center" colspan="4" rowspan="1">SOLICITADO POR:</td>
