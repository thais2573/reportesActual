    <td style="vertical-align: top; text-align: center" colspan="1" rowspan="4"><div style="width: 80px">VALE DE </div> <div style="width: 80px">ENTREGA #:</div></td>
    <td style="vertical-align: top; text-align: center" colspan="1" rowspan="4">SOLICITUD #:</br> {{ app.user.consecutivoSolicitud }}</td>
  </tr>
  <tr  style="height: 30px">
    <td colspan="4" rowspan="1">NOMBRE Y APELLIDOS: <div style="width: 200px">{{ lista.solicitante }}</div></td>
    <td colspan="4" rowspan="1">NOMBRE Y APELLIDOS:<div style="width: 200px"> </div></td>
