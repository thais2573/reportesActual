{% if editar == 'no' %}
  <div id="ent1" class="componente" data-prototype="{{ form_widget(form.componente.vars.prototype)|e }}" >
    {% for ent in form.componente %}
      {% set indice = loop.index-1 %}
      <div class="well text-info form-horizontal componente" id="componente_{{ indice }}">
        <div id="app_bundleequipo_fom_type_componente_{{ indice }}" class="container-fluid">
          <div class="form-group row">
            <div id="columna15_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              <label>Tipo de Componente</label>
                {{ form_errors(ent.tipoComponente) }}{{ form_widget(ent.tipoComponente) }}
              <div class="hidden">{{ form_label(ent.tipoComponente) }}</div>
            </div>
          </div>
          <div class="form-group row">
            <div id="columna1_{{ indice }}" class="col-md-6" style="padding-left: 5%;">
              <label>Modelo</label>
              {{ form_errors(ent.modelo) }}{{ form_widget(ent.modelo) }}
              <div class="hidden">{{ form_label(ent.modelo) }}</div>
            </div>
            <div id="columna2_{{ indice }}" class="col-md-6" style="padding-left: 5%;">
              <label>Serie</label>
              {{ form_errors(ent.serie) }}{{ form_label(ent.serie) }}{{ form_widget(ent.serie) }}
            </div>
            <div id="columna3_{{ indice }}" class="col-md-4" style="padding-left: 5%;">
              <label>Marca</label>
              {{ form_errors(ent.marca) }}{{ form_label(ent.marca) }}{{ form_widget(ent.marca) }}
            </div>
            <div id="columna4_{{ indice }}" class="col-md-4" style="padding-left: 5%;">
              {{ form_errors(ent.watts) }}{{ form_label(ent.watts) }}{{ form_widget(ent.watts) }}
            </div>
          </div>
          <div class="form-group row">
            <div id="columna5_{{ indice }}" class="col-md-4" style="padding-left: 5%;">
              {{ form_errors(ent.sata) }}{{ form_label(ent.sata) }}{{ form_widget(ent.sata) }}
            </div>
            <div id="columna6_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.capacidad) }}{{ form_label(ent.capacidad) }}{{ form_widget(ent.capacidad) }}
            </div>
            <div id="columna7_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.velocidad) }}{{ form_label(ent.velocidad) }}{{ form_widget(ent.velocidad) }}
            </div>
            <div id="columna8_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.lga) }}{{ form_label(ent.lga) }}{{ form_widget(ent.lga) }}
            </div>
          </div>
          <div class="form-group row">
            <div id="columna9_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.ram) }}{{ form_label(ent.ram) }}{{ form_widget(ent.ram) }}
            </div>

            <div id="columna11_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.optico) }}{{ form_label(ent.optico) }}{{ form_widget(ent.optico) }}
            </div>
            <div id="columna12_{{ indice }}" class="col-md-3" style="padding-left: 5%;">
              {{ form_errors(ent.conector) }}{{ form_label(ent.conector) }}{{ form_widget(ent.conector) }}
            </div>
            {#<div id="columna13_{{ indice }}" class="col-md-3" style="padding-left: 5%;">#}
              {#{{ form_errors(ent.tipo) }}{{ form_label(ent.tipo) }}{{ form_widget(ent.tipo) }}#}
            {#</div>#}

          </div>
        </div>
      </div>
      <BR/>
    {% endfor %}
  </div>
